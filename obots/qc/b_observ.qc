void (entity client) CycleClientCamera = 
{
    local entity tofollow;
    
    if(client.classname != "observer")
    {
        sprint(client, "not in observer mode\n");
        return;
    }
    
    if(client.camera == client)
    {
        client.clientorigin = client.origin;
        client.clientv_angle = client.v_angle;
        tofollow = find(world, classname, "player");
        
        if(tofollow)
        {
            
            if(tofollow == client)
            {
                tofollow = find(tofollow, classname, "player");
            }
            
            
            if(tofollow)
            {
                client.camera = tofollow;
            }
            
        }
        
        
        if(client.camera == client)
        {
            sprint(client, "no players to view from\n");
            tofollow = find(world, classname, "bot");
            if(tofollow)
            {
                client.camera = tofollow;
            }
            else
            {
                sprint(client, "no bots to view from\n");
            }
            
        }
        
    }
    else
    {
        if((client.camera.classname == "player") || (client.camera.classname == "observer"))
        {
            tofollow = find(client.camera, classname, "player");
            
            if(tofollow)
            {
                
                if(tofollow == client)
                {
                    tofollow = find(tofollow, classname, "player");
                }
                
                
                if(tofollow)
                {
                    client.camera = tofollow;
                }
                
            }
            
            
            if(!tofollow)
            {
                tofollow = find(world, classname, "bot");
                if(tofollow)
                {
                    client.camera = tofollow;
                }
                else
                {
                    client.camera = client;
                }
                
            }
            
        }
        else
        {
            tofollow = find(client.camera, classname, "bot");
            if(tofollow)
            {
                client.camera = tofollow;
            }
            else
            {
                client.camera = client;
            }
            
        }
        
    }
    
    if(client.camera == client)
    {
        func002357(client);
    }
    else
    {
        setsize(client, VEC_ORIGIN, VEC_ORIGIN);
    }
    
};


